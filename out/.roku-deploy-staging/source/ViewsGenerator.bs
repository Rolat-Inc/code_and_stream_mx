function openTab(newTabId as string, parentNode as object )
	viewsContainer = initViewsContainer(newTabId)

	parentNode.insertChild(viewsContainer, 1)

	'Set focus to new tab
	applyFocusTo(viewsContainer, newTabId, parentNode)
end function

'Init views wrapper
function initViewsContainer(newTabId as string) as dynamic
	viewsContainer = m.top.createChild("ViewsContainer")
	viewsContainer.id = "ViewsContainer"
	viewsContainer.tabId = newTabId

	return viewsContainer
end function